   "source": [
    "year = int(input(\"Please enter a year: \"))\n",
    "\n",
    "leap= (year % 4 == 0) and ((year % 400 == 0) or (year % 100 != 0))\n",
    "\n",
    "if leap==True :\n",
    "\n",
    "    print(year, \"is a leap year\")\n",
    "else: \n",
    "    print(year, \"is not a leap year\")"
