   "source": [
    "prime = []\n",
    "\n",
    "n = int(input(\"enter a limit number\"))\n",
    "\n",
    "for num in range(2, n+1):\n",
    "    status = True\n",
    "    for i in range(2, num):\n",
    "        if num %i ==0: status=False\n",
    "    if status:\n",
    "        prime.append(num)\n",
    "\n",
    "print(prime)\n",
    "    \n",
